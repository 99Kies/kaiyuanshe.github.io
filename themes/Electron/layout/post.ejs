<section class="page-section blog-post">
  <div class="container-narrow">
    <h1><%= page.title %></h1>

    <% if (page.description) { %>
      <blockquote class="blockquote">
          <p class="mb-0"><%= page.description %></p>
      </blockquote>
    <% } %>

    <%- include('./byline', { post: page }) %>

    <%- page.content %>
  </div>
</section>
